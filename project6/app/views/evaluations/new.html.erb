<h1>Evaluations#new</h1>
<p>Find me in app/views/evaluations/new.html.erb</p>
<%= render 'form', evaluation: @evaluation %>
<%= link_to 'BackToIndex', evaluations_index_path %>


<table border="1">
    <tr>
        <td> First Name </td> <td> Last Name </td>  <td> Role </td> <td> ID </td> <td> Email </td>
    </tr>
        
    <% User.all.each do |user| %>
        <tr>
            <td> <%= user.fname %> </td>
            <td> <%= user.lname %> </td>
            <td>
                <% if user.instructor %>
                    Instructor
                <% else %>
                    Student
                <% end %>
            </td>
            <td> <%= user.id %> </td>

            <td> <%= user.email %> </td>
        </tr>
    <% end %>

</table>